{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Constraints",
  "type": "object",
  "properties": {
    "stringProperty": {
      "type": "string",
      "minLength": 3,
      "maxLength": 10
    },
    "integerProperty": {
      "type": "integer",
      "minimum": 1,
      "maximum": 100
    },
    "arrayProperty": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "minItems": 2,
      "maxItems": 5
    }
  },
  "required": ["stringProperty", "integerProperty", "arrayProperty"]
}